  min-height: 100vh;
  justify-content: center;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.img-style {
  margin: 0px;
  height: 200px;
  width: 200px;
}

.img-container {
  justify-content: center;
  display: flex;
  flex-direction: row;
  align-items: center;
}
